<%= simple_form_for cargo, remote: true, url: fixture_path, html: { method: :patch, id: :update_fixture } do |f| %>

  <div class="row" id="discharging">
    <div class="col col1">
      <%= f.input :disch_port,
                    input_html: {readonly: true, class: "form-control-sm edit-input"},
                    label: "Port",
                    required: true %>

    </div>
    <div class="col col2">
      <%= f.input :disch_terminal,
                    input_html: {readonly: true, class: "form-control-sm edit-input"},
                    label: "Terminal",
                    required: true %>

    </div>
    <div class="col col3">
      <%= f.input :disch_berth,
                    input_html: {readonly: true, class: "form-control-sm edit-input"},
                    label: "Berth",
                    required: true %>
    </div>
    <div class="col col4">
      <%= f.input :disch_obl, as: :fake,
                    input_html: {readonly: true, class: "form-control-sm edit-input"},
                    label: "Obl",
                    required: true %>
    </div>
    <div class="col col5">
      <%= f.input :disch_name, as: :fake,
                    input_html: {readonly: true, class: "form-control-sm edit-input"},
                    label: "Name",
                    required: true %>
    </div>
    <div class="col col6">
      <%= f.input :disch_quantity_mts, as: :fake,
                    input_html: {readonly: true, class: "form-control-sm edit-input"},
                    label: "Qty - Mts",
                    required: true %>
    </div>
  </div>
<% end %>
